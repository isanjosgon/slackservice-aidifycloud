{
	"templates": {
		"pullrequestevent-opened" : {
			"icon_url": "https://raw.githubusercontent.com/isanjosgon/slackservice-aidifycloud/master/resources/images/logo.png",
			"username": "ai{D}fy message",
			"attachments": [
				{
					"fallback": "New pull request to check from @{{eventUser}}",
					"color": "#FFA74D",
					"fields": [
						{
							"title": "Description",
							"value": "New pull request to check from @{{eventUser}}\ntitle: \"{{title}}\"\nbody: \"{{body}}\"\n<{{link}} | Go to the pull request>",
							"short": false
						},
						{
							"title": "Repo",
							"value": "{{repo}}",
							"short": true
						}
					],
					"thumb_url": "https://raw.githubusercontent.com/isanjosgon/slackservice-aidifycloud/master/resources/images/new-message.png"
				}
			]
		},
		"pullrequestevent-reopened" : {
			"icon_url": "https://raw.githubusercontent.com/isanjosgon/slackservice-aidifycloud/master/resources/images/logo.png",
			"username": "ai{D}fy message",
			"attachments": [
				{
					"fallback": "@{{eventUser}} reopen a pull request",
					"color": "#FFA74D",
					"fields": [
						{
							"title": "Description",
							"value": "Reopened pull request to check from @{{eventUser}}\ntitle: \"{{title}}\"\nbody: \"{{body}}\"\n<{{link}} | Go to the pull request>",
							"short": false
						},
						{
							"title": "Repo",
							"value": "{{repo}}",
							"short": true
						}
					],
					"thumb_url": "https://raw.githubusercontent.com/isanjosgon/slackservice-aidifycloud/master/resources/images/new-message.png"
				}
			]
		},
		"pullrequestevent-closed" : {
			"icon_url": "https://raw.githubusercontent.com/isanjosgon/slackservice-aidifycloud/master/resources/images/logo.png",
			"username": "ai{D}fy message",
			"attachments": [
				{
					"fallback": "@{{eventUser}} closed your pull request",
					"color": "#D77793",
					"fields": [
						{
							"title": "Description",
							"value": "@{{eventUser}} closed your pull request\ntitle: \"{{title}}\"\nbody: \"{{body}}\"\n<{{link}} | Go to the pull request>",
							"short": false
						},
						{
							"title": "Repo",
							"value": "{{repo}}",
							"short": true
						},
						{
							"title": "Points lost",
							"value": "{{points}}",
							"short": true
						}
					],
					"thumb_url": "https://raw.githubusercontent.com/isanjosgon/slackservice-aidifycloud/master/resources/images/dismissed.png"
				}
			]
		},
		"pullrequestevent-merged" : {
			"icon_url": "https://raw.githubusercontent.com/isanjosgon/slackservice-aidifycloud/master/resources/images/logo.png",
			"username": "ai{D}fy message",
			"attachments": [
				{
					"fallback": "@{{eventUser}} merged your pull request",
					"color": "#6BAD6B",
					"fields": [
						{
							"title": "Description",
							"value": "@{{eventUser}} merged your pull request\ntitle: \"{{title}}\"\nbody: \"{{body}}\"\n<{{link}} | Go to the pull request>",
							"short": false
						},
						{
							"title": "Repo",
							"value": "{{repo}}",
							"short": true
						},
						{
							"title": "Points earned",
							"value": "{{points}}",
							"short": true
						}
					],
					"thumb_url": "https://raw.githubusercontent.com/isanjosgon/slackservice-aidifycloud/master/resources/images/accepted.png"
				}
			]
		}
	}
}